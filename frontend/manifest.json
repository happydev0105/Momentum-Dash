{
    "manifest_version": 3,
    "name": "Vision Board Extension",
    "version": "1.0",
    "description": "A personalized vision board similar to Momentum Dash.",
    "permissions": ["storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "icons/icon.png"
    },
    "icons": {
      "48": "icons/icon.png"
    }
  }
  